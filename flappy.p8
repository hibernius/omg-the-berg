-- starfield

stars = {
	init=function(self)
		self.data={}
		self.colors={5,6,7}
		self.dx=3
		if menu.mode==3 then
			self.colors={5,5,7}
			self.dx=1
		end
	end,
	update=function(self)
		self:star_gen()
		for i,s in pairs(self.data) do
			s.x-=s.ci*self.dx
			-- delete old stars
			if s.x<-1 then
				if (flap.alive and s.ci==3) stats.savoid+=1
				del(self.data,self.data[i])
			end
		end
	end,
	draw=function(self,x,y,c)
		for s in all(self.data) do
			col=self.colors[s.ci]
			if s.ci==2 and rnd(1)>0.9 then
				col=flr(rnd(15)+1)
			end
			rectfill(s.x,s.y,s.x,s.y,col)
		end
	end,
	star_gen=function(self)
		do_star=rnd(1)
		if do_star>0.5 and flap.alive then
			self:add_star()
		end
	end,
	add_star=function(self,x,y)
		add(self.data,{
			x=x or 127, -- x coord
			y=y or rnd(127), -- y coord
			ci=flr(rnd(#self.colors))+1, -- colors index
			st=t -- start time
		})
	end
}
