-- time to get home

ttgh={
	active=true,
	update=function(self)
		if (not self.active) return
		-- decrement
		if (t%30==0)	self.t.s-=1
		-- cascading decrement
		if (self.t.s==-1) self.t.m-=1
		if (self.t.m==-1) self.t.h-=1
		if (self.t.h==-1) self.t.d-=1
		if (self.t.d==-1) self.t.y-=1
		-- reset timers
		if (self.t.s==-1) self.t.s=59
		if (self.t.m==-1) self.t.m=59
		if (self.t.h==-1) self.t.h=23
		if (self.t.d==-1) self.t.d=self.t.y%4 and 364 or 365 -- leap years are important
	end,
	get=function(self)
		--print(self.t.y.." years "..self.t.d.." days",0,117,7)
		--print(self.t.h.." hours "..self.t.m.." minutes "..self.t.s.." seconds",0,123,7)
		local tl=self.t.y.."y "..self.t.d.."d "..self.t.h.."h "..self.t.m.."m "..self.t.s.."s"
		return tl
	end,
	init=function(self)
		self.t={
			y=75,
			d=0,
			h=0,
			m=0,
			s=0
		}
		self.active=true
	end,
	are_we_there_yet=function(self)
		return self.t.y==0 and
									self.t.d==0 and
									self.t.h==0 and
									self.t.m==0 and
									self.t.s==0
	end
}
