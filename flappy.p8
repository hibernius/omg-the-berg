-- flappy

flap={
	ws=9.975, -- warp speed
	f=0, -- flap factor
	ss={16,17}, -- sprites,
	spm={}, -- collidable pixels
	alive=true,
	tod=0, -- time of death
	update=function(self)
	if self.alive then
			self:set_spm()
			self:check()
		else
			self.ws=0
		end
		self:flap()
	end,
	draw=function(self)
		if self.alive then
			local s = self.ss[1]
			if self.f>0 or not self.alive then
				s = self.ss[flr(t/5)%#self.ss+1]
			end
			spr(s,self.x,self.y)
		end
	end,
	check=function(self)
		if (btn(üÖæÔ∏è))	self.f=8
		--if (btnp(üÖæÔ∏è)) sfx(35)
		if (btnp(‚ùé)) ptorps:fire(self.x,self.y)
	end,
	flap=function(self)
		if self.f>0 then
			if self.y>self.y_min then
				self.y-=1
			end
			self.f-=1
		else
			if (self.y<self.y_max) self.y+=1
		end
	end,
	is_flappy=function(self)
		return self.f>0
	end,
	set_spm=function(self)
		self.spm={}
		for y=0,8 do
			for x=0,8 do
				if pget(flap.x+x,flap.y+y)!=0 then
					add(self.spm,{x=flap.x+x,y=flap.y+y})
				end
			end
		end
	end,
	res=function(self)
		self.alive=true
		self.x=16
		self.y=64
		self.y_min=8 -- upper bound
		self.y_max=112 -- lower bound
		self.ws=9.975
		self.tod=0
	end
}
