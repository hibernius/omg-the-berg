-- collisions

function flaps_collision()
	for c in all(berg.ships) do
		if flap.alive and c.alive then
			for p in all(flap.spm) do
				if p.x > c.x and
						p.x < c.x+16 and
						p.y > c.y and
						p.y < c.y+16
				then
					flap.alive=false
					flap.tod=t
					ttgh.active=false
					berg.active=false
					pgen:set_pos(flap.x+3,flap.y+5)
					pgen:generate(50,{5,6,8,10,12})
					music(-1)
					sfx(27)
				end
			end
		end
	end
end

function ptorps_collision()
	for c in all(berg.ships) do
		for p in all(ptorps.a) do
			if p.x+4 > c.x and
						p.x+4 < c.x+16 and
						p.y+4 > c.y and
						p.y+4 < c.y+16
			then
				c.alive=false
				p.active=false
				stats.bkill+=1
				pgen:set_pos(p.x+4,p.y+4)
				pgen:generate(100,{3,5,11})
				sfx(37)
				return
			end
		end
	end
end

function btorps_collision()
	for a in all(btorps.a) do
		for p in all(flap.spm) do
			if flr(a.x)==p.x and
						flr(a.y)==p.y
			then
				flap.alive=false
				flap.tod=t
				a.active=false
				ttgh.active=false
				berg.active=false
				pgen:set_pos(flap.x+3,flap.y+5)
				pgen:generate(50,{5,6,8,10,12})
				music(-1)
				sfx(27)
				return
			end
		end
	end
end

function stars_collision()
	for s in all(stars.data) do
		if s.ci==3 then
			for i in all(flap.spm) do
				if flr(s.x)==i.x and
						flr(s.y)==i.y
				then
					flap.alive=false
					flap.tod=t
					ttgh.active=false
					pgen:set_pos(flap.x+3,flap.y+5)
					pgen:generate(50,{5,6,8,10,12})
					music(-1)
					sfx(27)
					return
				end
			end
		end
	end
end
