stats={
	draw=function(self)
		ptorps_str="torpedoes launched: "..self.ptorps..self:get_suf(self.ptorps)
		bkill_str="berg murdered: "..self.bkill..self:get_suf(self.bkill)
		bspare_str="berg spared: "..self.bspare..self:get_suf(self.bspare)
		meff_str="murder efficiency: "..self:get_me(self.ptorps,self.bkill).."%"..self:get_meff(self.ptorps,self.bkill)
		savoid_str="stars avoided: "..self.savoid..self:get_suf(self.savoid)
		col=t>flap.tod+30 and 7 or 5
		if menu.mode==3 then
			print(savoid_str,hcenter(savoid_str),64,col)
		else
			print(ptorps_str,hcenter(ptorps_str),48,col)
			print(bkill_str,hcenter(bkill_str),56,col)
			print(bspare_str,hcenter(bspare_str),64,col)
			print(meff_str,hcenter(meff_str),72,col)
		end
	end,
	get_meff=function(self,a,b)
		if (a==0 or b==0) return ""
		eff=(b/a)*100
		str=""
		if (eff<100 and eff!=flr(eff)) str=" (ish)"
		if (eff==69) str=" (nice)"
		return str
	end,
	get_me=function(self,a,b)
		return (a==0 or b==0) and 0 or flr((b/a)*100)
	end,
	get_suf=function(self,val)
		val=""
		if (val==69) val=" (nice)"
		return val
	end,
	res=function(self)
		self.bkill=0
		self.bspare=0
		self.ptorps=0
		self.savoid=0
	end
}
