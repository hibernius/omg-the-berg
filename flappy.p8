-- particle generator

pgen={
	x=0,
	y=0,
	particles={},
	update=function(self)
		for i,p in pairs(self.particles) do
			p.x+=p.dx
			p.y+=p.dy
			if p.x<0 or p.x>128 or
					p.y<0 or p.y>128
			then
				del(self.particles,self.particles[i])
			end
		end
	end,
	draw=function(self)
		for p in all(self.particles) do
			rectfill(p.x,p.y,p.x,p.y,p.c)
		end
	end,
	set_pos=function(self,x,y)
		self.x=x
		self.y=y
	end,
	generate=function(self,maxp,cols)
		for i=1,maxp do
			self:add_particle(cols)
		end
	end,
	add_particle=function(self,cols)
		dadd=fiddy() and 2 or -2
		add(self.particles,{
			x=self.x,
			y=self.y,
			dx=self:gen_dp(4)+dadd,
			dy=self:gen_dp(4)+dadd,
			c=cols[flr(rnd(#cols)+1)]
		})
	end,
	gen_dp=function(self,maxd)
		dp=rnd(maxd)
		return fiddy() and dp or dp-dp*2
	end,
	res=function(self)
		self.particles={}
	end
}
