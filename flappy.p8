function _update_game()
	
	if (t==1) scenario.active=true
	
	if scenario.active then
		berg.active=false
		_upd=_update_scenes
		_drw=_draw_scenes
	end

	stars:update()	
	flap:update()
	flaps_collision()
	ptorps:update()
	pgen:update()
	ttgh:update()
	if menu.mode==1 then
		berg:update()
		ptorps_collision()
	end
	if menu.mode==2 then
		berg:update()
		btorps:update()
		ptorps_collision()
		btorps_collision()
	end
	if menu.mode==3 then
		stars:update()
		stars_collision()
	end
	hud:update()
	if not flap.alive and t>flap.tod+30 then	
		if (btnp(‚ùé) or btnp(üÖæÔ∏è)) _init_title()
	end
	
	-- check win condition
