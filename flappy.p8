	can_move={
		left=false,
		right=false
	},
	update=function(self)
		if (self.active==false) return
		if self.mselect=="main" then
			if btnp(üÖæÔ∏è) then
				if self.option==1 then
					self.mselect="diff"
					sfx(41,2)
				elseif (self.option==2) then
					_init_tutorial()
				elseif (self.option==3) then
					_init_credits()
				end
			end
			if btnp(‚¨ÜÔ∏è) and self.option > 1 then
				self.option-=1
				sfx(41,2)
			end
			if btnp(‚¨áÔ∏è) and self.option < #self.options then
				self.option+=1
				sfx(41,2)
			end
		elseif self.mselect=="diff" then
			-- select mode
			if btnp(üÖæÔ∏è) then
				_init_game()
			end
			-- handle mode select
			if btnp(‚¨ÖÔ∏è) and self.mode>1 then
				self.mode-=1
				sfx(41,2)
			end
			if btnp(‚û°Ô∏è) and self.mode<#self.options[self.option].options then
				self.mode+=1
				sfx(41,2)
			end
			-- mode select direction check
			self.can_move.left=self.mode>1
			self.can_move.right=self.mode<#self.options[self.option].options
			-- return to main options
			if btnp(‚¨ÜÔ∏è) or btnp(‚¨áÔ∏è) or btnp(‚ùé) then
				self.mselect="main"
				sfx(42,2)
				return
			end
		end
	end,
	draw=function(self)
		if (self.active==false) return
		local y,pre,pst,str=0,"","",""
		for k,v in pairs(self.options) do
			if self.mselect=="diff" and v.options then
				pre=self.can_move.left and "< " or ""
				pst=self.can_move.right and " >" or ""
				str=pre..v.options[self.mode].s..pst
			else
				pre="> "
				pst=" <"
				str=k==self.option and pre..v.s..pst or v.s
			end
			print(str,hcenter(str),self.y+y,10)
			y+=8
		end
	end,
	res=function(self)
		self.option=1
		self.mselect="main"
		self.active=false
	end
