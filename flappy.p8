-- photon torpedoes

ptorps={
	n=0, -- remaining torps (lol)
	m=38, -- max torps
	rc=false, -- retcon
	rct=0, -- retcon timer
	a={}, -- active torps
	v=4, -- speed
	ss={18,19}, -- sprites
	cool=0, -- cooldown
	update=function(self)
		for i,a in pairs(self.a) do
			a.x+=self.v
			-- delete old torps
			if a.x<-8 or not a.active then
				del(self.a,self.a[i])
			end
		end
		-- cooldown
		if (self.cool>0) self.cool-=1
		self.tc = self.cool>0 and 1 or 12
		-- retcon torps
		if not self.rc and self.n==0 then
			self.rc=true
			self.n=0
			self.rct=50
		end
		if (self.rct>0) self.rct-=1
		if flap.alive and self.rc and t%100==0 and self.n<self.m then
			self.n+=1
		end
	end,
	draw=function(self)
		palt(0,true)
		for a in all(self.a) do
			spr(self.ss[flr(t/1)%#self.ss+1],a.x,a.y)
		end
		palt()
		if self.rc and self.rct>0 then
			if t%4==0 then
				print("retcon activated",64,8,12)
			end
		end
	end,
	fire=function(self,vx,vy)
		if (self.cool>0 or self.n<=0) return
		add(self.a,{
			x=vx,
			y=vy,
			active=true
		})
		self.n-=1
		self.cool=30
		stats.ptorps+=1
		sfx(36)
	end,
	res=function(self)
		self.n=self.m
		self.rc=false
		self.rct=0
		self.a={}
		self.cool=0
	end
}

btorps={
	init=function(self)
		self.a={}
		self.ss={20,21}
	end,
	update=function(self)
		for i,a in pairs(self.a) do
			a.x+=a.dx
			a.y+=a.dy
			-- delete old torps
			if a.x<0 or a.x>128 or
						a.y<0 or a.y>128 or
						not a.active
			then
				del(self.a,self.a[i])
			end
		end
	end,
	draw=function(self)
		palt(0,true)
		for a in all(self.a) do
			spr(self.ss[flr(t/1)%#self.ss+1],a.x,a.y)
		end
		palt()
	end,
	fire=function(self,x,y)
		add(self.a,{
			x=x,
			y=y,
			dx=-4,
			dy=self:ch_dir(y,rnd(2)),
			active=true
		})
		sfx(36)
	end,
	ch_dir=function(self,y,dy)
		return flap.y>y and dy or dy-(dy*2)
	end
}
