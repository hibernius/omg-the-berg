-- omg the berg

berg={
	active=false,
	ships={},
	cool=0,
	update=function(self)
		if rnd(1)>0.9 and self.cool<=0 and self.active then
			self:add_cube()
			self.cool=20
		end
		for i,c in pairs(self.ships) do
			c.x-=c.v
			-- fire!
			if self:can_fire(c) then
				btorps:fire(c.x,c.y)
				c.fire=true
			end
			-- delete old cubes
			if c.x<-16 then
				del(self.ships,self.ships[i])
				if (flap.alive) stats.bspare+=1
			end
			-- destroy cube
			if not c.alive and c.dc==0 then
				del(self.ships,self.ships[i])
			end
		end
		self.cool-=1
	end,
	draw=function(self)
		palt(0,false)
		for c in all(self.ships) do
			sspr(c.sx,c.sy,c.sw,c.sh,c.x,c.y,c.w,c.h,c.fx,c.fy)
		end
		palt()
	end,
	add_cube=function(self)
		-- set v based on difficulty
		v=1
		add(self.ships,{
			v=rnd(3)+v,
			alive=true,
			dc=0,
			--sx=rnd(48)+80,
			--sy=rnd(16),
			sx=self.ssx[flr(rnd(2)+1)],
			sy=self.ssy[flr(rnd(1)+1)],
			sw=16,
			sh=16,
			x=128,
			y=rnd(104),
			w=16,
			h=16,
			fx=rnd(1)>0.5,
			fy=rnd(1)>0.5,
			fire=false,
		})
	end,
	can_fire=function(self,c)
		return menu.mode==2 and
									not c.fire and
									c.x<rnd(116) and
									c.x>flap.x+8
	end,
	res=function(self)
		self.active=true
		self.ships={}
		self.cool=0
		self.ssx={80,104,120}
		self.ssy={0,16}
	end
}
